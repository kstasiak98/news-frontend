mputed: {\r\n    cart() {\r\n      return this.$store.state.cart;\r\n    }\r\n  },\r\n  methods: {\r\n    make_object_array(arrray) {\r\n        for (let i=0; i< arrray.length; i += 1) {\r\n            if(arrray[i] != null) {\r\n                for (let j=0; j< arrray[i].miejsca.length; j += 1) {\r\n                    const Data = {\r\n                        client: \"user1\",\r\n                        row: arrray[i].miejsca[j].lay,\r\n                        column: arrray[i].miejsca[j].number,\r\n                        filmName: this.cart[0].seans.seans_film,\r\n                    }\r\n                    this.sendTicket(Data);\r\n                }\r\n            }\r\n        }\r\n    },\r\n\r\n    sendTicket(Data) {\r\n      // let endpoint = \"api/ticket/\";\r\n      // let method = \"POST\";\r\n      // // const testData = {\r\n      // //   client: \"user\",\r\n      // //   row: \"A\",\r\n      // //   column: 10,\r\n      // //   seans_name: 1\r\n      // // };\r\n      // apiService(method, endpoint, {\r\n      //   client: \"user\",\r\n      //   row: \"A\",\r\n      //   column: 10,\r\n      //   seans_name: 1\r\n      // })\r\n      //   .then(function(response) {\r\n      //     console.log(response);\r\n      //   })\r\n      //   .catch(function(error) {\r\n      //     console.log(error);\r\n      //   });\r\n      axios.post(\r\n          \"/api/ticket/\",\r\n\r\n           {\r\n              client: Data.client,\r\n                row: Data.row,\r\n                column: Data.column,\r\n                seans_name: Data.filmName\r\n          },{\r\n          headers: {\"X-CSRFTOKEN\": CSRF_TOKEN}\r\n      })\r\n        .then(function(response) {\r\n          console.log(response);\r\n        })\r\n        .catch(function(error) {\r\n          console.log(error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"sourceRoot":"src/components/shop"}]}